generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

/// /////////
/// /////////
model user {
  id         Int    @id @default(autoincrement())
  email      String @unique @db.VarChar(255)
  password   String
  firstName  String @db.VarChar(255)
  lastName   String @db.VarChar(255)
  phone      String
}
